<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 内部颜色 -->
    <!--默认图形大小-->
    <size
        android:width="100dp"
        android:height="100dp" />
    <solid
        android:color="#66333333" />
    <!-- 边缘线条颜色 -->
    <stroke android:color="#66333333"
        android:width="1dp"
        />
    <gradient android:startColor="#66333333"
        android:endColor="#66333333"/>
    <!-- 圆角的幅度 -->
    <corners android:radius="100dp"/>
</shape>

