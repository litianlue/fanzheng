<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 内部颜色 -->
    <!--默认图形大小-->
    <size
        android:width="750dp"
        android:height="100dp" />
    <solid
        android:color="@color/white" />
    <!-- 边缘线条颜色 -->
    <stroke android:color="#19d6c4"
        android:width="4dp"
        />
    <gradient android:startColor="#19d6c4"
        android:endColor="#19d6c4"/>
    <!-- 圆角的幅度 -->
    <corners android:radius="100dp"/>
</shape>

